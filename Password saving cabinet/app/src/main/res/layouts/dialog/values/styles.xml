<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="rental_textView_context">
        <item name="android:layout_marginEnd">3dp</item>
        <item name="android:minWidth">80dp</item>
        <item name="android:textSize">@dimen/rental_lable_textSize</item>
    </style>

    <style name="rental_textView_Lable">
        <item name="android:textSize">@dimen/rental_lable_textSize</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="rental_add_edit">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textSize">@dimen/rental_lable_textSize</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">fill_horizontal</item>
    </style>

    <dimen name="rental_lable_textSize">18dp</dimen>
</resources>